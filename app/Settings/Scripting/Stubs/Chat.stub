
Eloquent('Chat', function (modelClass) {

    return class extends modelClass {

        static endpoint = "<endpoint>";
        static scopes = [<scopes>];
        static dates = [<dates>];

	    notify() {
	        return this.connection.sendRequest(this.id + '/notify', 'get');
	    }

    };

});
<globalObject> = Eloquent.Chat;

