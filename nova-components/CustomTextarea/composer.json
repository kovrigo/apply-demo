{
    "name": "kovrigo/custom-textarea",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Kovrigo\\CustomTextarea\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kovrigo\\CustomTextarea\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
